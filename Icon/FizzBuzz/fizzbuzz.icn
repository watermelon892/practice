# Memo : Run with sudo
# $ sudo ./fizzbuzz

procedure fizzbuzz(i)
    if i % 15 = 0 then
        return "FizzBuzz"
    else if i % 3 = 0 then
        return "Fizz"
    else if i % 5 = 0 then
        return "Buzz"
    else
        return i
end

procedure run(n)
    every i := 1 to n do
        write(fizzbuzz(i))
end

procedure main()
    run(15)
end
